<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/verde_fundo"
    tools:context=".TelaLogin">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="283dp"
        android:layout_height="246dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_11" />

    <TextView
        android:id="@+id/linkesqsenha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="8dp"
        android:autoLink="web"
        android:fontFamily="@font/carme"
        android:letterSpacing="0.01"
        android:linksClickable="true"
        android:text="esqueci minha senha"
        android:textColor="@color/link"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/editTextsenha"
        app:layout_constraintTop_toBottomOf="@+id/editTextsenha" />

    <TextView
        android:id="@+id/textemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/carme"
        android:letterSpacing="0.01"
        android:text="Email"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextEmail"
        app:layout_constraintLeft_toLeftOf="@+id/editTextEmail" />

    <EditText
        android:id="@+id/editTextsenha"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="316dp"
        android:paddingStart="15dp"
        android:background="@drawable/inputemailsenha"
        android:fontFamily="@font/carme"
        android:hint=" Digite sua senha"
        android:inputType="textEmailAddress"
        android:textColor="#60030C0C"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextEmail"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="396dp"
        android:paddingStart="15dp"
        android:background="@drawable/inputemailsenha"
        android:fontFamily="@font/carme"
        android:hint="Digite seu email"
        android:inputType="textEmailAddress"
        android:textColor="#60030C0C"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.7"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textsenha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/carme"
        android:letterSpacing="0.01"
        android:text="Senha"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextsenha"
        app:layout_constraintStart_toStartOf="@+id/editTextsenha" />


    <Button
        android:id="@+id/btentrar"
        android:layout_width="364dp"
        android:layout_height="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="468dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/button_green"
        android:fontFamily="@font/carme"
        android:letterSpacing="0.1"
        android:text="Entrar"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textSize="15sp"
        app:backgroundTint="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/linkcad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="160dp"
        android:layout_marginTop="632dp"
        android:autoLink="web"
        android:fontFamily="@font/carme"
        android:letterSpacing="0.01"
        android:linksClickable="true"
        android:text="Cadastre-se"
        android:textAlignment="center"
        android:textColor="@color/link"
        android:textSize="15sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="192dp"
        android:layout_marginTop="592dp"
        android:fontFamily="@font/carme"
        android:letterSpacing="0.01"
        android:text="ou"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btgoogle"
        android:layout_width="364dp"
        android:layout_height="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/button_google_icon"
        android:fontFamily="@font/carme"
        android:letterSpacing="0.1"
        android:text="Login via Google"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:backgroundTint="@null"
        app:layout_constraintCircleRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btentrar" />

</androidx.constraintlayout.widget.ConstraintLayout>